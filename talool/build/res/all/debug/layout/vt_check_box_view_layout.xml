<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <CheckBox
        android:id="@+id/saveToVenmoTouchView_checkbox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="true" />

    <TextView
        android:id="@+id/saveToVenmoTouchView_text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/saveToVenmoTouchView_checkbox"
        android:text="@string/consent_text_preamble" />

    <TextView
        android:id="@+id/saveToVenmoTouchView_howItWorks"
        android:textColor="@color/blue_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/saveToVenmoTouchView_text"
        android:layout_toRightOf="@+id/saveToVenmoTouchView_checkbox"
        android:text="@string/consent_text_how_it_works"
        android:layout_marginRight="8sp"/>

    <TextView
        android:id="@+id/saveToVenmoTouchView_termsOfService"
        android:textColor="@color/blue_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/saveToVenmoTouchView_text"
        android:layout_toRightOf="@+id/saveToVenmoTouchView_howItWorks"
        android:text="@string/consent_text_terms_of_service"
        android:layout_marginRight="8sp" />

    <TextView
        android:id="@+id/vtSaveToVenmoTouchView_email"
        android:textColor="@color/blue_link"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/saveToVenmoTouchView_text"
        android:layout_toRightOf="@+id/saveToVenmoTouchView_termsOfService"
        android:text="@string/vt_consent_text_email" />
    
</RelativeLayout><!-- From: file:/Users/zachmanc/Documents/workspace/VenmoTouch/venmoTouch/src/main/res/layout/vt_check_box_view_layout.xml --><!-- From: file:/Users/zachmanc/Documents/workspace/talool-mobile-android-gradle/talool/build/exploded-bundles/ComBraintreeAndroidVenmoTouch100B5.aar/res/layout/vt_check_box_view_layout.xml -->